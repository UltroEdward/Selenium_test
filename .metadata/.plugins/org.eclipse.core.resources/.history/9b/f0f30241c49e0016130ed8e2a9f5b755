public class BasePage {

	private WebDriver driver = null;

	public BasePage(WebDriver driver) {
		this.driver = driver;
		
	}
	
	private void waitForPageToLoad(String xpathToWait){
		WebDriverWait wait = new WebDriverWait(driver, 30)
		
	}

}

package com.manahealth.sv.selenium.ui.tests.framework.steps.app;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.support.PageFactory;

import com.manahealth.sv.selenium.ui.tests.framework.driver.DriverFactory;
import com.manahealth.sv.selenium.ui.tests.framework.pages.app.HomePage;
import com.manahealth.sv.selenium.ui.tests.framework.pages.app.LoginPage;
import com.manahealth.sv.selenium.ui.tests.framework.steps.BaseStep;

public class LoginStep extends BaseStep{
	
	LoginPage loginPage = PageFactory.initElements(driver, LoginPage.class);
	
	
	public LoginStep(WebDriver driver) {
		super(driver);
	}

	public HomePage login(){
		
		return new HomePage(driver);
	}

}
